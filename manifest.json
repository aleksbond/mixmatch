{
    "manifest_version": 2,
    "name": "MixMatch",
    "version": "0.1",
		"content_scripts": [
			{
				"matches": ["https://www.modcloth.com/*"],
				"js": ["content.js"]
			}
		],
		"browser_action": {
			"default_icon": "icon.png",
			"default_popup": "popup.html"
		},
		"background": {
			"scripts": ["background.js"]
		}
}
